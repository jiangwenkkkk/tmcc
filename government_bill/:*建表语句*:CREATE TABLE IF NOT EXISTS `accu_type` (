/*建表语句*/CREATE TABLE IF NOT EXISTS `accu_type` (  `accu_type_id` bigint(10) NOT NULL,  `tariff_unit_id` int(9) NOT NULL,  `can_be_negative` tinyint(1) NOT NULL,  `accu_direction` tinyint(1) NOT NULL,  `accu_type_name` varchar(100) NOT NULL,  `can_send` tinyint(1) NOT NULL,  `accu_desc` varchar(250) DEFAULT NULL,  `region_id` int(9) DEFAULT NULL,  `status_cd` varchar(10) NOT NULL,  `create_staff` bigint(16) DEFAULT NULL,  `update_staff` bigint(16) DEFAULT NULL,  `create_date` datetime DEFAULT NULL,  `update_date` datetime DEFAULT NULL,  `status_date` datetime DEFAULT NULL) ENGINE=InnoDB DEFAULT CHARSET=utf8;/*sharding语句*/sharding @@table name='accu_type' set type='single' and dn='mvno_comm2_01';


/*建表语句*/CREATE TABLE IF NOT EXISTS `attr_spec` (  `attr_id` bigint(16) NOT NULL,  `busi_type_id` bigint(16) DEFAULT NULL,  `par_attr_id` bigint(16) DEFAULT NULL,  `attr_nbr` varchar(30) DEFAULT NULL,  `attr_name` varchar(60) DEFAULT NULL,  `attr_desc` varchar(2000) DEFAULT NULL,  `default_value` varchar(30) DEFAULT NULL,  `value_from` varchar(30) DEFAULT NULL,  `value_to` varchar(250) DEFAULT NULL,  `is_unique` tinyint(2) DEFAULT NULL,  `is_nullable` tinyint(2) DEFAULT NULL,  `attr_value_data_type` varchar(4) DEFAULT NULL,  `is_dany_attr` tinyint(2) DEFAULT NULL,  `attr_value_type` varchar(4) DEFAULT NULL,  `attr_format` varchar(2000) DEFAULT NULL,  `attr_length` int(9) DEFAULT NULL,  `status_cd` varchar(10) NOT NULL,  `create_staff` bigint(16) DEFAULT NULL,  `update_staff` bigint(16) DEFAULT NULL,  `create_date` datetime DEFAULT NULL,  `status_date` datetime DEFAULT NULL,  `update_date` datetime DEFAULT NULL,  `remark` varchar(250) DEFAULT NULL,  `ext_attr_id` varchar(32) DEFAULT NULL,  `grp_attr_nbr` varchar(30) DEFAULT NULL,  `attr_type_old` tinyint(2) DEFAULT NULL,  `attr_id_old` bigint(10) DEFAULT NULL,  PRIMARY KEY (`attr_id`),  UNIQUE KEY `PK_ATTR_SPEC` (`attr_id`),  KEY `RELATIONSHIP_30_FK` (`busi_type_id`),  KEY `RELATIONSHIP_34_FK` (`par_attr_id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8;/*sharding语句*/sharding @@table name='attr_spec' set type='single' and dn='mvno_comm2_01';

