  `pub_type` tinyint(1) NOT NULL,
  `pub_date` datetime NOT NULL,
  `notes` varchar(512) DEFAULT NULL,
  `pub_state` varchar(3) NOT NULL,
  `staff_id` int(9) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8  
CREATE TABLE `b_param_define` (
  `param_segment` varchar(20) NOT NULL,
  `param_code` varchar(64) NOT NULL,
  `param_value` varchar(1000) DEFAULT NULL,
  `param_name` varchar(100) DEFAULT NULL,
  `remark` varchar(100) DEFAULT NULL,
  `host_id` int(9) NOT NULL,
  `sys_username` varchar(50) NOT NULL,
  `cloud_flag` tinyint(2) DEFAULT NULL,
  `org_id` int(9) DEFAULT NULL,
  `level_flag` varchar(20) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8  
CREATE TABLE `b_userinfo_process_status` (
  `node_group` tinyint(1) DEFAULT NULL,
  `userinfo_flag` tinyint(1) DEFAULT NULL,
  `userinfo_state` tinyint(1) DEFAULT NULL,
  `process_state` tinyint(1) DEFAULT NULL,
  `state_date` datetime DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8  
CREATE TABLE `wf_application` (
  `APP_ID` decimal(8,0) NOT NULL,
  `EXEC_NAME` varchar(100) NOT NULL,
  `DESCRIPTION` varchar(80) DEFAULT NULL,
  `APP_TYPE` decimal(1,0) NOT NULL,
  `SQL_STATEMENT` varchar(2000) DEFAULT NULL,
  `IF_NEED_FORMAT` decimal(1,0) NOT NULL DEFAULT '0',
  `ORG_ID` decimal(9,0) DEFAULT NULL,
  `MODULE_ID` decimal(3,0) DEFAULT NULL,
  `DOMAIN_TYPE` varchar(10) DEFAULT NULL,
  `PROCESS_TYPE` varchar(10) DEFAULT NULL,
  `LIFE_CYCLE_TYPE` varchar(10) DEFAULT NULL,
  `BUSINESS_TYPE` varchar(10) DEFAULT NULL,
  `EXE_PATH` varchar(255) DEFAULT NULL,
  `EXE_NAME` varchar(255) DEFAULT NULL,
  `STATIC_PARAMS` varchar(512) DEFAULT NULL,
  `DYNAMIC_PARAMS` varchar(512) DEFAULT NULL,
  `DISPLAY_NAME` varchar(255) DEFAULT NULL,
  `LIB_PATH` varchar(255) DEFAULT NULL,
  `PROGRAM_DESC` varchar(512) DEFAULT NULL,
  `PROGRAM_GROUP` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`APP_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8  
