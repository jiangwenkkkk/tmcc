rejectfile	CREATE TABLE `rejectfile` (\n  `id` varchar(50) NOT NULL DEFAULT '' COMMENT 'id',\n  `item_type` varchar(10) NOT NULL DEFAULT '' COMMENT 'LNIG  LNDG ',\n  `err_code` varchar(10) NOT NULL DEFAULT '' COMMENT '错误码',\n  `file_name` varchar(64) NOT NULL DEFAULT '' COMMENT '文件名',\n  `file_size` int(10) NOT NULL DEFAULT '-1' COMMENT '文件大小',\n  `file_create_time` varchar(32) NOT NULL DEFAULT '' COMMENT '文件创建时间',\n  `log_time` bigint(20) NOT NULL DEFAULT '-1' COMMENT '入库时间',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='拒收文件表'
rejectfile_history	CREATE TABLE `rejectfile_history` (\n  `id` varchar(50) NOT NULL DEFAULT '' COMMENT 'id',\n  `item_type` varchar(10) NOT NULL DEFAULT '' COMMENT 'LNIG  LNDG ',\n  `err_code` varchar(10) NOT NULL DEFAULT '' COMMENT '错误码',\n  `file_name` varchar(64) NOT NULL DEFAULT '' COMMENT '文件名',\n  `file_size` int(10) NOT NULL DEFAULT '-1' COMMENT '文件大小',\n  `file_create_time` varchar(32) NOT NULL DEFAULT '' COMMENT '文件创建时间',\n  `log_time` bigint(20) NOT NULL DEFAULT '-1' COMMENT '入库时间',\n  `sharding_id` int(8) NOT NULL COMMENT '分片',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='拒收文件表'
tp_area	CREATE TABLE `tp_area` (\n  `area_code` int(4) NOT NULL,\n  `prov_id` int(3) NOT NULL,\n  `area_name` varchar(120) NOT NULL,\n  `english_name` varchar(54) NOT NULL,\n  `valid_time` date NOT NULL,\n  `elapse_time` datetime NOT NULL,\n  `update_time` datetime NOT NULL,\n  `modifier` varchar(30) NOT NULL,\n  `audit_time` datetime NOT NULL,\n  `auditor` varchar(30) NOT NULL,\n  `hash_value` bigint(32) NOT NULL,\n  PRIMARY KEY (`area_code`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
wf_hostinfo	CREATE TABLE `wf_hostinfo` (\n  `HOST_ID` int(5) NOT NULL,\n  `HOST_IP` varchar(45) NOT NULL,\n  `HOST_NAME` varchar(120) NOT NULL,\n  `ORG_ID` bigint(12) NOT NULL,\n  `BEFORE_HOST_ID` int(5) NOT NULL,\n  `ROWID` int(9) NOT NULL,\n  PRIMARY KEY (`ROWID`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
