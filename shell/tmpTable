hss_order_log	CREATE TABLE `hss_order_log` (\n  `T_DATE` datetime NOT NULL,\n  `SESSION_ID` varchar(200) NOT NULL DEFAULT '',\n  `TABLE_NAME` varchar(200) NOT NULL DEFAULT '',\n  `T_NUM` bigint(10) NOT NULL,\n  `HASH_DATA` text NOT NULL,\n  `ROWID` bigint(12) NOT NULL AUTO_INCREMENT,\n  PRIMARY KEY (`ROWID`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
hss_order_log_err	CREATE TABLE `hss_order_log_err` (\n  `T_DATE` datetime NOT NULL,\n  `SESSION_ID` varchar(100) NOT NULL DEFAULT '',\n  `ERR_ID` int(8) NOT NULL,\n  `ERR_DB_ID` int(8) NOT NULL,\n  `ERR_SQL` text NOT NULL,\n  `ERR_MSG` text NOT NULL,\n  `ERR_TABLE_NAME` varchar(100) NOT NULL DEFAULT '',\n  `ERR_XML` text NOT NULL,\n  `ROWID` bigint(12) NOT NULL AUTO_INCREMENT,\n  PRIMARY KEY (`ROWID`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
ids	CREATE TABLE `ids` (\n  `ID` int(9) NOT NULL,\n  PRIMARY KEY (`ID`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
logic_statement	CREATE TABLE `logic_statement` (\n  `CONDITION_ID` int(9) NOT NULL,\n  `STATEMENT_SEQ` int(9) NOT NULL,\n  `PARAM_FUNCTION` int(4) NOT NULL,\n  `PARAM1_ID` varchar(31) NOT NULL DEFAULT '',\n  `LEFT_PARAM1` varchar(31) NOT NULL DEFAULT '',\n  `PARAM2_ID` varchar(31) NOT NULL DEFAULT '',\n  `LEFT_PARAM2` varchar(31) NOT NULL DEFAULT '',\n  `PARAM3_ID` varchar(31) NOT NULL DEFAULT '',\n  `LEFT_PARAM3` varchar(31) NOT NULL DEFAULT '',\n  `OPERATOR` int(3) NOT NULL,\n  `CONDITION_VAL_ID` varchar(31) NOT NULL DEFAULT '',\n  `CONDITION_VALUE` varchar(31) NOT NULL DEFAULT '',\n  `GROUP_ID` int(9) NOT NULL,\n  `TEMPLATE_CONDITION_ID` int(9) NOT NULL,\n  `TEMPLATE_STATEMENT_SEQ` int(9) NOT NULL,\n  `CREATE_STAFF_ID` bigint(12) NOT NULL,\n  `CREATE_DATE` bigint(14) NOT NULL,\n  `ORI_CONDITION_ID` int(9) NOT NULL,\n  `ORI_STATEMENT_SEQ` int(9) NOT NULL,\n  `ROWID` bigint(12) NOT NULL AUTO_INCREMENT,\n  PRIMARY KEY (`ROWID`),\n  KEY `idx_logic_statement_condition_id` (`CONDITION_ID`) USING BTREE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
message_alert	CREATE TABLE `message_alert` (\n  `alert_id` varchar(50) NOT NULL COMMENT '告警ID',\n  `mode_code` varchar(125) NOT NULL COMMENT '模块名',\n  `bol_name` varchar(125) NOT NULL COMMENT 'bol名字',\n  `alert_type` int(8) NOT NULL COMMENT '告警类型',\n  `ALERT_TIME` int(8) NOT NULL COMMENT '告警时间',\n  `alert_text` varchar(512) NOT NULL COMMENT '告警内容',\n  `sharding_id` int(8) NOT NULL COMMENT '分片',\n  `state` int(2) NOT NULL DEFAULT '0',\n  PRIMARY KEY (`alert_id`),\n  KEY `idx_message_alert_mode_code` (`mode_code`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
