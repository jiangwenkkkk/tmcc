errfiletodb	CREATE TABLE `errfiletodb` (\n  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'id',\n  `item_type` int(4) NOT NULL DEFAULT '0' COMMENT '1:文件  2:bill 3:E开头文件的文件信息',\n  `err_code` varchar(10) NOT NULL DEFAULT '0' COMMENT '错误码',\n  `file_name` varchar(64) NOT NULL DEFAULT '0' COMMENT '文件名',\n  `file_size` int(10) NOT NULL DEFAULT '0' COMMENT '文件大小',\n  `file_create_time` varchar(32) NOT NULL DEFAULT '0' COMMENT '文件创建时间',\n  `record_type` int(10) NOT NULL DEFAULT '0' COMMENT '记录类型',\n  `error_line` int(10) NOT NULL DEFAULT '0' COMMENT '错误行号',\n  `org_recod` varchar(1025) NOT NULL DEFAULT '0' COMMENT '原错误记录',\n  `vist_office` varchar(10) NOT NULL DEFAULT '0' COMMENT '拜访局',\n  `ower_office` varchar(10) NOT NULL DEFAULT '0' COMMENT '归属局',\n  `file_serial` varchar(10) NOT NULL DEFAULT '0' COMMENT '文件序号',\n  `file_produce_time` varchar(20) NOT NULL DEFAULT '0' COMMENT '文件生产日期',\n  `file_version` int(10) NOT NULL DEFAULT '0' COMMENT '文件版本号',\n  `error_count` int(10) NOT NULL DEFAULT '0' COMMENT '错单数',\n  `proc_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '处理时间',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8
